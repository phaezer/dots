{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "Lua.workspace.library": ["${3rd}/luv/library", "${3rd}/busted/library"],
  "Lua.workspace.checkThirdParty": false,
  "Lua.runtime.version": "LuaJIT",
  "Lua.runtime.special": {
    "vim": "require"
  },
  "Lua.diagnostics.globals": ["vim", "Snacks", "MiniDeps"],
    "diagnostics.disable": [
        "undefined-field"
    ]
}
